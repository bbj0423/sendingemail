<div class=container>
  <h1>메일보내기</h1>
  <form action ="/wow" method="POST">
    <div id= "A" class="form-group">
        <label for="exampleInputEmail1">제목</label>
        <input name= "title" type="text" class="form-control" id="exampleInputEmail1" placeholder="제목을써주세요">
    </div>
    
    <div id= "B" class="form-group">
        <label for="exampleInputEmail1">보내는 사람</label>
        <input name= "email" type="email" class="form-control" id="exampleInputEmail1" placeholder="이메일을써주세요">
    </div>
    
    
    <div class="form-group">
      <label for="exampleInputPassword1">글내용</label>
      <textarea name= "content" type='text' class="form-control" rows="5" id="comment" placeholder="글을써주세요" ></textarea>
    </div>
    <button type="submit" class="btn btn-default">보내기</button>
  </form>
</div>


